{
    "expo": {
        "name": "Bluesky Student",
        "slug": "bluesky-student",
        "version": "1.0.2",
        "orientation": "default",
        "userInterfaceStyle": "light",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#100D25"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "buildNumber": "5",
            "bundleIdentifier": "com.zetasolutionsonline.bluesky",
            "supportsTablet": true,
            "infoPlist": {
                "CFBundleAllowMixedLocalizations": true
            }
        },
        "android": {
            "versionCode": 3,
            "softwareKeyboardLayoutMode": "resize",
            "package": "com.zetasolutionsonline.bluesky",
            "jsEngine": "hermes",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/icon.png"
        },
        "plugins": [
            "sentry-expo",
            [
                "expo-notifications",
                {
                    "icon": "./assets//icon.png"
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "6d7b9344-5c7f-494e-bfbe-eb0c7678f86e"
            }
        }
    }
}
